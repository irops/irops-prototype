      "Properties" : {
        "UserName" : "mcrawford",
        "Path" : "/",
        "Groups" : [
          { "Fn::ImportValue" : { "Fn::Sub" : "${StandardGroupsStackName}-AdministratorsGroup" }},
          { "Fn::ImportValue" : { "Fn::Sub" : "${StandardGroupsStackName}-BastionUsersGroup" }}
        ]
      }
    },

